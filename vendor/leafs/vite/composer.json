{
  "name": "leafs/vite",
  "description": "Server component for Vite",
  "keywords": [
    "vite",
    "frontend",
    "leaf",
    "php",
    "framework"
  ],
  "homepage": "https://leafphp.dev/modules/views/vite/",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Michael Darko",
      "email": "mickdd22@gmail.com",
      "homepage": "https://mychi.netlify.app",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Leaf\\": "src"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "illuminate/support": "*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.14"
  },
  "scripts": {
    "format": "vendor/bin/php-cs-fixer fix --config=.php_cs.dist.php --allow-risky=yes"
  }
}
